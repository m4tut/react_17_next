{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "~assets": "./src/assets",
          "~entities": "./src/entities",
          "~features": "./src/features",
          "~pages": "./src/pages",
          "~processes": "./src/pages",
          "~shared": "./src/shared",
          "~widgets": "./src/widgets"
        }
      }
    ]
  ]
}
